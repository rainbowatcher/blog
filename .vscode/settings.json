{
    "eslint.experimental.useFlatConfig": true,
    "cSpell.words": [
        "akiravoid",
        "antfu",
        "Attributify",
        "bitnami",
        "Debezium",
        "demi",
        "getmeili",
        "gitea",
        "iconify",
        "intlify",
        "kafkaui",
        "kuma",
        "Meili",
        "meilisearch",
        "picocolors",
        "pinia",
        "pnpm",
        "provectuslabs",
        "ROWID",
        "shikijs",
        "unocss",
        "unplugin",
        "Vite",
        "vitejs",
        "Vitesse",
        "vitest",
        "vueuse"
    ],
    "i18n-ally.sourceLanguage": "zh-CN",
    "i18n-ally.keystyle": "nested",
    "i18n-ally.localesPaths": "src/locales",
    "i18n-ally.sortKeys": true,
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit",
        "source.organizeImports": "never"
    },
    "editor.formatOnSave": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "node_modules": true
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "vue",
        "html",
        "markdown",
        "json",
        "jsonc",
        "yaml",
        "toml",
        "gql",
        "graphql"
    ],
    "typescript.tsdk": "node_modules/typescript/lib"
}
