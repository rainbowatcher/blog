{
    "headers": [
        {
            "source": "/assets/(.*)\\.css",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
          "source": "/assets/(.*)\\.js",
          "headers": [
              {
                  "key": "Cache-Control",
                  "value": "public, max-age=31536000, immutable"
              }
          ]
      }
    ]
}
