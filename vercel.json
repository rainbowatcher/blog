{
    "headers": [
        {
            "source": "/assets/(index*.css)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000"
                },
                {
                    "key": "Expires",
                    "value": "3600"
                }
            ]
        }
    ]
}
