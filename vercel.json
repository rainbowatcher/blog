{
    "headers": [
        {
            "source": "/assets/(.*)\\.css",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=31536000,immutable"
                }
            ]
        },
        {
            "source": "/assets/(.*)\\.js",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=31536000,immutable"
                }
            ]
        },
        {
            "source": "/assets/(.*)\\.ttf",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=31536000,immutable"
                }
            ]
        }
    ]
}
